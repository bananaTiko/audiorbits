{
  "name": "audiorbits",
  "version": "2.4.0",
  "description": "Web Wallpaper for Wallpaper Engine",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean-win": "Rmdir /Q /S \"dist\"",
    "tsc": "tsc",
    "babel": "babel dist/tsc --out-dir dist/babel --source-maps",
    "webpack": "webpack",
    "pack": "cp -R public dist/pack",
    "pack-win": "xcopy public\\*.* dist\\pack\\ /e /i /y /s",
    "after-clean": "rm -rf ./dist/tsc",
    "after-clean-win": "Rmdir /Q /S \"dist\\tsc\"",
    "build-win": "yarn run clean-win && yarn run tsc && yarn run babel && yarn run webpack && yarn run pack-win && yarn run after-clean-win",
    "build": "yarn run clean && yarn run tsc && yarn run babel && yarn run webpack && yarn run pack && yarn run after-clean",
    "startdev": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexxone/audiorbits.git"
  },
  "author": "hexxone",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hexxone/audiorbits/issues"
  },
  "homepage": "https://github.com/hexxone/audiorbits#readme",
  "dependencies": {
    "@types/jquery": "^3.5.4",
    "jquery": "^3.5.1",
    "stats-js": "^1.0.1",
    "three": "^0.122.0"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "webpack": "^5.5.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
